<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.timeline.vpn.dao.db.CollectDao">
	<resultMap id="CollectMap" type="CollectPo">
		<id column="id" property="id" />
	</resultMap>
	<insert id="add">
		insert into
		vpn.collect(ip,count,day,hour,minute)
		values
		(#{ip},#{count},#{day},#{hour},#{minute})
	</insert>
</mapper>

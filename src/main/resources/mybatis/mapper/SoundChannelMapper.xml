<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.timeline.vpn.dao.db.SoundChannelDao">
	<resultMap id="SoundChannelMap" type="SoundChannel">
	</resultMap>
	<resultMap id="SoundItemsMap" type="SoundItems">
	</resultMap>
	<select id="getAll" resultMap="SoundChannelMap">
	    select * from vpn.soundchannel
	</select>
	<select id="getByChannel" resultMap="SoundItemsMap">
	    select * from vpn.sounditems where channel=#{channel} order by fileDate asc
	</select>
</mapper>

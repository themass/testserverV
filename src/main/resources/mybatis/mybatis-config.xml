<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>

	<typeAliases>
		<typeAlias alias="DevUseinfoPo" type="com.timeline.vpn.model.po.DevUseinfoPo" />
		<typeAlias alias="HostPo" type="com.timeline.vpn.model.po.HostPo" />
		<typeAlias alias="AppVersion" type="com.timeline.vpn.model.po.AppVersion" />
		<typeAlias alias="RecommendPo" type="com.timeline.vpn.model.po.RecommendPo" />
		<typeAlias alias="Version" type="com.timeline.vpn.model.po.AppVersion" />
		<typeAlias alias="LocationPo" type="com.timeline.vpn.model.po.LocationPo" />
		<typeAlias alias="UserPo" type="com.timeline.vpn.model.po.UserPo" />
		<typeAlias alias="IWannaPo" type="com.timeline.vpn.model.po.IWannaPo" />
		<typeAlias alias="RadCheck" type="com.timeline.vpn.model.po.RadCheck" />
		<typeAlias alias="RadUserGroup" type="com.timeline.vpn.model.po.RadUserGroup" />
		<typeAlias alias="DnsResverPo" type="com.timeline.vpn.model.po.DnsResverPo" />
		<typeAlias alias="Radacct" type="com.timeline.vpn.model.po.Radacct" />
		<typeAlias alias="RadacctState" type="com.timeline.vpn.model.po.RadacctState" />
		<typeAlias alias="CollectPo" type="com.timeline.vpn.model.po.CollectPo" />
		<typeAlias alias="SoundChannel" type="com.timeline.vpn.model.po.SoundChannel" />
		<typeAlias alias="SoundItems" type="com.timeline.vpn.model.po.SoundItems" />
		
		<typeAlias alias="TextChannelPo" type="com.timeline.vpn.model.po.TextChannelPo" />
		<typeAlias alias="TextItemsPo" type="com.timeline.vpn.model.po.TextItemsPo" />
		<typeAlias alias="TextItemPo" type="com.timeline.vpn.model.po.TextItemPo" />
		<typeAlias alias="FileIpPo" type="com.timeline.vpn.model.po.FileIpPo" />
		
		<typeAlias alias="ImgChannelPo" type="com.timeline.vpn.model.po.ImgChannelPo" />
		<typeAlias alias="ImgItemsPo" type="com.timeline.vpn.model.po.ImgItemsPo" />
		<typeAlias alias="ImgItemsItemPo" type="com.timeline.vpn.model.po.ImgItemsItemPo" />
		<typeAlias alias="BlackDevPo" type="com.timeline.vpn.model.po.BlackDevPo" />
		<typeAlias alias="VideoPo" type="com.timeline.vpn.model.po.VideoPo" />
	</typeAliases>
	<typeHandlers>
        <typeHandler javaType="String" jdbcType="BLOB"
            		 handler="com.timeline.vpn.dao.BlobStringTypeHandler"/>
    </typeHandlers>
	<plugins>
		<plugin interceptor="com.github.pagehelper.PageHelper">
			<property name="dialect" value="mysql" />
			<property name="pageSizeZero" value="true" />
			<property name="reasonable" value="true" />
		</plugin>
	</plugins>
</configuration>

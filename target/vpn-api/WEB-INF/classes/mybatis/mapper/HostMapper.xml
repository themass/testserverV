<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.timeline.vpn.dao.db.HostDao">
	<resultMap id="HostPoMap" type="HostPo">
		<id column="id" property="id" />
		<result column="gateway" property="gateway" />
		<result column="port" property="port" />
		<result column="location" property="location" />
		<result column="cert" property="cert" />
	</resultMap>
	<insert id="insert">
		insert into
		host(gateway,port,location,cert)
		values
		(#{gateway},#{port},#{location},#{cert})
	</insert>
	<select id="get" resultMap="HostPoMap">
		select * from host where id=#{id}
	</select>
	<select id="getAll" resultMap="HostPoMap">
		select * from host
	</select>
	<select id="getByLocation" resultMap="HostPoMap">
		select * from host where location=#{location}
	</select>
	
</mapper>

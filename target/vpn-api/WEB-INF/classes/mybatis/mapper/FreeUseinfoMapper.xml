<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.timeline.vpn.dao.db.FreeUseinfoDao">
	<resultMap id="FreeUseinfoMap" type="FreeUseinfoPo">
		<id column="devId" property="devId" />
		<result column="platform" property="platform" />
		<result column="version" property="version" />
		<result column="creatTime" property="creatTime" />
		<result column="lastUpdate" property="lastUpdate" />
	</resultMap>
	<insert id="insert">
		insert into
		freeuseinfo(devId,platform,version,creatTime,lastUpdate,useTime)
		values
		(#{devId},#{platform},#{version},#{creatTime},#{lastUpdate},#{useTime})
	</insert>
	<select id="get" resultMap="FreeUseinfoMap">
		select * from freeuseinfo where devId=#{devId}
	</select>
	<select id="getAll" resultMap="FreeUseinfoMap">
		select * from freeuseinfo
	</select>
	<update id="update">
	    update freeuseinfo 
			<set>
				<if test="lastUpdate != null">
					lastUpdate = #{lastUpdate},
				</if>
				<if test="version != null">
					version = #{version},
				</if> 
				<if test="platform != null">
					platform = #{platform},
				</if> 
				<if test="useTime != null">
					useTime = #{useTime},
				</if> 
			</set>
			where devId = #{devId}
	</update>
</mapper>

<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.timeline.vpn.dao.db.LocationDao">
	<resultMap id="LocationPoMap" type="LocationPo">
		<id column="id" property="id" />
		<result column="imag" property="imag" />
		<result column="name" property="name" />
		<result column="ename" property="ename" />
		<result column="type" property="type" />
		<result column="enable" property="enable" />
		<result column="cityName" property="cityName" />
		<result column="level" property="level" />
	</resultMap>

	<select id="getAll" resultMap="LocationPoMap">
		select * from vpn.location where enable=1
	</select>
</mapper>
